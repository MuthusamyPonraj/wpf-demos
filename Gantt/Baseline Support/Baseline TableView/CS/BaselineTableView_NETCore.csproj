<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <UseWPF>true</UseWPF>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <AssemblyName>BaselineTableView</AssemblyName>
    <RootNamespace>BaselineTableView</RootNamespace>
  </PropertyGroup>
  <ItemGroup>
    <Resource Include="App.ico" xmlns="http://schemas.microsoft.com/developer/msbuild/2003" />
    <Reference Include="Syncfusion.Gantt.WPF">
      <HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\netcoreapp3.0\Syncfusion.Gantt.WPF.dll</HintPath>
    </Reference>
    <Reference Include="Syncfusion.Grid.Wpf">
      <HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\netcoreapp3.0\Syncfusion.Grid.Wpf.dll</HintPath>
    </Reference>
    <Reference Include="Syncfusion.Linq.Base">
      <HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\netcoreapp3.0\Syncfusion.Linq.Base.dll</HintPath>
    </Reference>
    <Reference Include="Syncfusion.GridCommon.Wpf">
      <HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\netcoreapp3.0\Syncfusion.GridCommon.Wpf.dll</HintPath>
    </Reference>
    <Reference Include="Syncfusion.ProjIO.Base">
      <HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\netcoreapp3.0\Syncfusion.ProjIO.Base.dll</HintPath>
    </Reference>
    <Reference Include="Syncfusion.SampleLayout">
      <HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\netcoreapp3.0\Syncfusion.SampleLayout.dll</HintPath>
    </Reference>
    <Reference Include="Syncfusion.Shared.Wpf">
      <HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\netcoreapp3.0\Syncfusion.Shared.Wpf.dll</HintPath>
    </Reference>
    <Reference Include="Syncfusion.Licensing">
      <HintPath>$(SyncfusionInstallLocation)\precompiledassemblies\netcoreapp3.0\Syncfusion.Licensing.dll</HintPath>
    </Reference>
  </ItemGroup>
</Project>