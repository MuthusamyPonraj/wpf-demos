<Window x:Class="BIDashboard.Dialogs.RenameDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Rename Dialog" Width="300" Height="130"
        Icon="..\App.ico" WindowStartupLocation="CenterOwner">
    <Grid Margin="7">
        <Grid.RowDefinitions>
            <RowDefinition Height="23*" />
            <RowDefinition Height="25*" />
            <RowDefinition Height="35*" />
        </Grid.RowDefinitions>
        <TextBlock Grid.RowSpan="1" Height="23" HorizontalAlignment="Left" Name="textBlock1" Text="Name :" VerticalAlignment="Bottom" />
        <TextBox Grid.Row="1" Grid.RowSpan="1" Height="23" Name="textBox1" VerticalAlignment="Center" KeyDown="textBox1_KeyDown" />
        <Button Grid.Row="2" Height="23" HorizontalAlignment="Right" Name="button1" VerticalAlignment="Center" Width="75" Content="OK" Click="button1_Click" Margin="0,7,0,0" />
    </Grid>
</Window>
