<syncfusion:ChromelessWindow x:Class="CustomizationDemo.MainWindow" 
                           ResizeMode="CanResizeWithGrip" WindowStartupLocation="CenterScreen" UseNativeChrome="True"
                           xmlns:local="clr-namespace:CustomizationDemo"
