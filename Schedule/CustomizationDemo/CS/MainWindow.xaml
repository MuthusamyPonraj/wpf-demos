                                    <TextBlock   Text="Add"/>
                                </StackPanel>
                            </Border>
                        </radialmenu:SfRadialMenuItem.Header>
                    </radialmenu:SfRadialMenuItem>
                    <radialmenu:SfRadialMenuItem  Click="editButton_Click"  CheckMode="None"   >
                        <radialmenu:SfRadialMenuItem.Header>
