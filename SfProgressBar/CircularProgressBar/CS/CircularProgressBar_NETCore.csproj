<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <UseWPF>true</UseWPF>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <AssemblyName>CircularProgressBar</AssemblyName>
    <RootNamespace>CircularProgressBar</RootNamespace>
  </PropertyGroup>
  <ItemGroup>
    <Resource Include="Resources\App.ico" xmlns="http://schemas.microsoft.com/developer/msbuild/2003" />
    <Resource Include="Resources\SfProgressPause.png" xmlns="http://schemas.microsoft.com/developer/msbuild/2003" />
    <Resource Include="Resources\SfProgressPlay.png" xmlns="http://schemas.microsoft.com/developer/msbuild/2003" />
    <PackageReference Include="Syncfusion.SfProgressBar.WPF" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.Licensing" Version="17.3.0.14" />
  </ItemGroup>
</Project>