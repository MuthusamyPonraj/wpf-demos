                                            <Path Fill="Transparent" Stroke="Gray" StrokeThickness="1" Stretch="Fill" Data="M 10 0 L 40 0 L 30 40 L 0 40 Z"/>
                                        </Button>
                                        <TextBlock Width="2"></TextBlock>
                                    </WrapPanel>
                                </Border>
                            </Grid>

                        </panels:SelectorPanel>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

</ResourceDictionary>