<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <UseWPF>true</UseWPF>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <AssemblyName>FloorPlanner</AssemblyName>
    <RootNamespace>FloorPlanner</RootNamespace>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="PresavedFloorPlan\FloorPlan.xml" />
    <None Remove="PresavedFloorPlan\FloorPlan1.xml" />
  </ItemGroup>
  <ItemGroup>
    <Content Include="PresavedFloorPlan\FloorPlan.xml">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\..\DiagramFrontPageUtility\DiagramFrontPageUtility\DiagramFrontPageUtility_NETCore.csproj" />
  </ItemGroup>
  <ItemGroup>
    <Resource Include="App.ico" />
    <Content Include="PresavedFloorPlan\FloorPlan1.xml">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <PackageReference Include="Syncfusion.Licensing" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.SfColorPalette.Wpf" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.SfDiagram.WPF" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.SfInput.Wpf" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.SfRadialMenu.Wpf" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.SfShared.Wpf" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.Shared.WPF" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.Tools.Wpf" Version="17.3.0.14" />
  </ItemGroup>
</Project>