<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <UseWPF>true</UseWPF>
    <UseWindowsForms>true</UseWindowsForms>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <AssemblyName>FromulaAnalysis</AssemblyName>
    <RootNamespace>FromulaAnalysis</RootNamespace>
  </PropertyGroup>
  <ItemGroup>
    <Resource Include="**\*.png" />
    <None Remove="App.ico" />
    <PackageReference Include="Syncfusion.Licensing" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.Calculate.Base" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.Grid.Wpf" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.GridCommon.WPF" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.Licensing" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.Linq.Base" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.Shared.WPF" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.Tools.Wpf" Version="17.3.0.14" />
  </ItemGroup>
  <ItemGroup>
    <Content Include="App.ico">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>
  <ItemGroup>
    <None Update="CalcEngine.xml">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>
</Project>