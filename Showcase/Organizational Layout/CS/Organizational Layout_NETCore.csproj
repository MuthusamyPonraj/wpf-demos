<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <UseWPF>true</UseWPF>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <AssemblyName>ExpenseAnalysisDemo</AssemblyName>
    <RootNamespace>ExpenseAnalysisDemo</RootNamespace>
  </PropertyGroup>
  <ItemGroup>
    <Resource Include="**\*.png" />
    <None Remove="Employee.xml" />
    <Content Include="Employee.xml">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Resource Include="App.ico" />
    <PackageReference Include="Syncfusion.Licensing" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.SfDiagram.WPF" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.SfInput.Wpf" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.SfShared.Wpf" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.Shared.WPF" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.Tools.Wpf" Version="17.3.0.14" />
  </ItemGroup>
</Project>