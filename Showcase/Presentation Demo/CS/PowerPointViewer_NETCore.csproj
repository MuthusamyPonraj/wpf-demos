<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <UseWPF>true</UseWPF>
    <UseWindowsForms>true</UseWindowsForms>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <AssemblyName>PowerPointViewer_2013</AssemblyName>
    <RootNamespace>PowerPointViewer_2013</RootNamespace>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DefineConstants>TRACE;NETCORE</DefineConstants>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DefineConstants>TRACE;NETCORE</DefineConstants>
  </PropertyGroup>
  <ItemGroup>
    <Resource Include="**\*.png" />
    <Resource Include="**\*.jpg" />
    <Resource Include="**\*.ico" />
    <None Remove="Data\NewCharts.pptx" />
    <Content Include="Data\NewCharts.pptx">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <PackageReference Include="Syncfusion.Compression.Base" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.Licensing" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.OfficeChart.Base" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.OfficeChartToImageConverter.WPF" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.Pdf.Wpf" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.Presentation.Wpf" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.PresentationToPdfConverter.Wpf" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.SfChart.WPF" Version="17.3.0.14" />
    <PackageReference Include="Syncfusion.Shared.WPF" Version="17.3.0.14" />
  </ItemGroup>
</Project>