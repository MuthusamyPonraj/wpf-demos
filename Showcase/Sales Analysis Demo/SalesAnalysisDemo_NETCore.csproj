    <None Remove="ShapeFile\Europe.shp" />
    <None Remove="ShapeFile\NorthAmerica.dbf" />
    <None Remove="ShapeFile\NorthAmerica.shp" />
    <None Remove="ShapeFile\Oceania.dbf" />
    <None Remove="ShapeFile\Oceania.shp" />
    <None Remove="ShapeFile\SouthAmerica.dbf" />
    <None Remove="ShapeFile\SouthAmerica.shp" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="ShapeFile\Africa.dbf">
