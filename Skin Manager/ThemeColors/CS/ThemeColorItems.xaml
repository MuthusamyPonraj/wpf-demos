<UserControl x:Class="ThemeColorsDemo.ThemeColorItems"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" xmlns:syncfusion="http://schemas.syncfusion.com/wpf">
    <ItemsControl>
        <Border Background="{DynamicResource {x:Static syncfusion:ThemeColors.HoverBrush}}" Tag="HoverBrush" Width="107" Height="30" BorderBrush="Black" BorderThickness="0 1 1 1"/>
        <Border Background="{DynamicResource {x:Static syncfusion:ThemeColors.BackgroundBrush}}" Tag="BackgroundBrush" Width="107" Height="30" BorderBrush="Black" BorderThickness="0 0 1 1"/>
        <Border Background="{DynamicResource {x:Static syncfusion:ThemeColors.BorderBrush}}" Width="107" Tag="BorderBrush}" Height="30" BorderBrush="Black" BorderThickness="0 0 1 1"/>
        <Border Background="{DynamicResource {x:Static syncfusion:ThemeColors.SelectedBrush}}" Width="107" Height="30" Tag="SelectedBrush" BorderBrush="Black" BorderThickness="0 0 1 1"/>
        <Border Background="{DynamicResource {x:Static syncfusion:ThemeColors.ForegroundBrush}}" Width="107" Height="30" Tag="ForegroundBrush" BorderBrush="Black" BorderThickness="0 0 1 1"/>
        <Border Background="{DynamicResource {x:Static syncfusion:ThemeColors.ActiveWindowTitleBrush}}" Width="107" Height="30" Tag="ActiveWindowTitleBrush" BorderBrush="Black" BorderThickness="0 0 1 1"/>
        <Border Background="{DynamicResource {x:Static syncfusion:ThemeColors.InActiveWindowTitleBrush}}" Width="107" Height="30" Tag="InActiveWindowTitleBrush" BorderBrush="Black" BorderThickness="0 0 1 1"/>
        <Border Background="{DynamicResource {x:Static syncfusion:ThemeColors.RibbonPanelBrush}}" Width="107" Height="30" Tag="RibbonPanelBrush" BorderBrush="Black" BorderThickness="0 0 1 1"/>
        <Border Background="{DynamicResource {x:Static syncfusion:ThemeColors.MenuBackgroundBrush}}" Width="107" Height="30" Tag="MenuBackgroundBrush" BorderBrush="Black" BorderThickness="0 0 1 1"/>
        <Border Background="{DynamicResource {x:Static syncfusion:ThemeColors.MenuHoverBrush}}" Width="107" Height="30" Tag="MenuHighlightBrush" BorderBrush="Black" BorderThickness="0 0 1 1"/>
        <Border Background="{DynamicResource {x:Static syncfusion:ThemeColors.GalleryHeaderBrush}}" Width="107" Height="30" Tag="GalleryHeaderBrush" BorderBrush="Black" BorderThickness="0 0 1 1"/>
        <Border Background="{DynamicResource {x:Static syncfusion:ThemeColors.ColumnHeaderBrush}}" Width="107" Height="30" Tag="ColumnHeaderBrush" BorderBrush="Black" BorderThickness="0 0 1 1"/>
        <Border Background="{DynamicResource {x:Static syncfusion:ThemeColors.CardViewGroupingBarBrush}}" Width="107" Height="30" Tag="CardViewGroupingBarBrush" BorderBrush="Black" BorderThickness="0 0 1 1"/>
        <Border Background="{DynamicResource {x:Static syncfusion:ThemeColors.TabHoverBrush}}" Width="107" Height="30" Tag="TabHoverBrush" BorderBrush="Black" BorderThickness="0 0 1 1"/>
        <Border Background="{DynamicResource {x:Static syncfusion:ThemeColors.TabHoverBorderBrush}}" Width="107" Height="30" Tag="TabHoverBorderBrush" BorderBrush="Black" BorderThickness="0 0 1 1"/>
        <Border Background="{DynamicResource {x:Static syncfusion:ThemeColors.TrackBrush}}" Width="107" Height="30" Tag="TrackBrush" BorderBrush="Black" BorderThickness="0 0 1 1"/>
        <Border Background="{DynamicResource {x:Static syncfusion:ThemeColors.HorizontalThumbBrush}}" Width="107" Height="30" Tag="HorizontalThumbBrush" BorderBrush="Black" BorderThickness="0 0 1 1"/>
        <Border Background="{DynamicResource {x:Static syncfusion:ThemeColors.HorizontalThumbHoverBrush}}" Width="107" Height="30" Tag="HorizontalThumbHoverBrush" BorderBrush="Black" BorderThickness="0 0 1 1"/>
        <Border Background="{DynamicResource {x:Static syncfusion:ThemeColors.HorizontalThumbPressedBrush}}" Width="107" Height="30" Tag="HorizontalThumbPressedBrush" BorderBrush="Black" BorderThickness="0 0 1 1"/>
        <Border Background="{DynamicResource {x:Static syncfusion:ThemeColors.VerticalThumbBrush}}" Width="107" Height="30" Tag="VerticalThumbBrush" BorderBrush="Black" BorderThickness="0 0 1 1"/>
        <Border Background="{DynamicResource {x:Static syncfusion:ThemeColors.VerticalThumbHoverBrush}}" Width="107" Height="30" Tag="VerticalThumbHoverBrush" BorderBrush="Black" BorderThickness="0 0 1 1"/>
        <Border Background="{DynamicResource {x:Static syncfusion:ThemeColors.VerticalThumbPressedBrush}}" Width="107" Height="30" Tag="VerticalthumbPressedBrush" BorderBrush="Black" BorderThickness="0 0 1 1"/>
    </ItemsControl>
</UserControl>
