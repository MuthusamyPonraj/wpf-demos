<UserControl x:Class="ThemeColorsDemo.ThemeColorTexts"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" xmlns:syncfusion="http://schemas.syncfusion.com/wpf">
    <ItemsControl>
        <Border Width="180" Height="30" BorderBrush="Black" BorderThickness="0 1 1 1">            
            <TextBox IsReadOnly="True" Style="{x:Null}" IsTabStop="False" BorderThickness="0" Text="HoverBrush" VerticalAlignment="Center" Margin="5"/>
        </Border>
        <Border Width="180" Height="30" BorderBrush="Black" BorderThickness="0 0 1 1">
            <TextBox IsReadOnly="True" Style="{x:Null}" IsTabStop="False" BorderThickness="0" Text="BackgroundBrush" VerticalAlignment="Center" Margin="5"/>
        </Border>
        <Border Width="180" Height="30" BorderBrush="Black" BorderThickness="0 0 1 1">
            <TextBox IsReadOnly="True" Style="{x:Null}" IsTabStop="False" BorderThickness="0" Text="BorderBrush" VerticalAlignment="Center" Margin="5"/>
        </Border>
        <Border Width="180" Height="30" BorderBrush="Black" BorderThickness="0 0 1 1">
            <TextBox IsReadOnly="True" Style="{x:Null}" IsTabStop="False" BorderThickness="0" Text="SelectedBrush" VerticalAlignment="Center" Margin="5"/>
        </Border>
        <Border Width="180" Height="30" BorderBrush="Black" BorderThickness="0 0 1 1">
            <TextBox IsReadOnly="True" Style="{x:Null}" IsTabStop="False" BorderThickness="0" Text="Foreground Brush" VerticalAlignment="Center" Margin="5"/>
        </Border>
        <Border Width="180" Height="30" BorderBrush="Black" BorderThickness="0 0 1 1">
            <TextBox IsReadOnly="True" Style="{x:Null}" IsTabStop="False" BorderThickness="0" Text="ActiveWindowTitleBrush" VerticalAlignment="Center" Margin="5"/>
        </Border>
        <Border  Width="180" Height="30" BorderBrush="Black" BorderThickness="0 0 1 1">
            <TextBox IsReadOnly="True" Style="{x:Null}" IsTabStop="False" BorderThickness="0" Text="InactiveWindowTitleBrush" VerticalAlignment="Center" Margin="5"/>
        </Border>
        <Border  Width="180" Height="30" BorderBrush="Black" BorderThickness="0 0 1 1">
            <TextBox IsReadOnly="True" Style="{x:Null}" IsTabStop="False" BorderThickness="0" Text="RibbonPanelBrush" VerticalAlignment="Center" Margin="5"/>
        </Border>
        <Border  Width="180" Height="30" BorderBrush="Black" BorderThickness="0 0 1 1">
            <TextBox IsReadOnly="True" Style="{x:Null}" IsTabStop="False" BorderThickness="0" Text="MenuBrush" VerticalAlignment="Center" Margin="5"/>
        </Border>
        <Border Width="180" Height="30" BorderBrush="Black" BorderThickness="0 0 1 1">
            <TextBox IsReadOnly="True" Style="{x:Null}" IsTabStop="False" BorderThickness="0" Text="MenuHighlightBrush" VerticalAlignment="Center" Margin="5"/>
        </Border>
        <Border Width="180" Height="30" BorderBrush="Black" BorderThickness="0 0 1 1">
            <TextBox IsReadOnly="True" Style="{x:Null}" IsTabStop="False" BorderThickness="0" Text="GalleryHeaderBrush" VerticalAlignment="Center" Margin="5"/>
        </Border>
        <Border  Width="180" Height="30" BorderBrush="Black" BorderThickness="0 0 1 1">
            <TextBox IsReadOnly="True" Style="{x:Null}" IsTabStop="False" BorderThickness="0" Text="ColumnHeaderBrush" VerticalAlignment="Center" Margin="5"/>
        </Border>
        <Border Width="180" Height="30" BorderBrush="Black" BorderThickness="0 0 1 1">
            <TextBox IsReadOnly="True" Style="{x:Null}" IsTabStop="False" BorderThickness="0" Text="CardViewGroupingBarBrush" VerticalAlignment="Center" Margin="5"/>
        </Border>
        <Border Width="180" Height="30" BorderBrush="Black" BorderThickness="0 0 1 1">
            <TextBox IsReadOnly="True" Style="{x:Null}" IsTabStop="False" BorderThickness="0" Text="TabHoverBrush" VerticalAlignment="Center" Margin="5"/>
        </Border>
        <Border  Width="180" Height="30" BorderBrush="Black" BorderThickness="0 0 1 1">
            <TextBox IsReadOnly="True" Style="{x:Null}" IsTabStop="False" BorderThickness="0" Text="TabHoverBorderBrush" VerticalAlignment="Center" Margin="5"/>
        </Border>
        <Border Width="180" Height="30" BorderBrush="Black" BorderThickness="0 0 1 1">
            <TextBox IsReadOnly="True" Style="{x:Null}" IsTabStop="False" BorderThickness="0" Text="TrackBrush" VerticalAlignment="Center" Margin="5"/>
        </Border>
        <Border Width="180" Height="30" BorderBrush="Black" BorderThickness="0 0 1 1">
            <TextBox IsReadOnly="True" Style="{x:Null}" IsTabStop="False" BorderThickness="0" Text="HorizontalThumbBrush" VerticalAlignment="Center" Margin="5"/>
        </Border>
        <Border Width="180" Height="30" BorderBrush="Black" BorderThickness="0 0 1 1">
            <TextBox IsReadOnly="True" Style="{x:Null}" IsTabStop="False" BorderThickness="0" Text="HorizontalThumbHoverBrush" VerticalAlignment="Center" Margin="5"/>
        </Border>
        <Border Width="180" Height="30" BorderBrush="Black" BorderThickness="0 0 1 1">
            <TextBox IsReadOnly="True" Style="{x:Null}" IsTabStop="False" BorderThickness="0" Text="HorizontalThumbPressedBrush" VerticalAlignment="Center" Margin="5"/>
        </Border>
        <Border Width="180" Height="30" BorderBrush="Black" BorderThickness="0 0 1 1">
            <TextBox IsReadOnly="True" Style="{x:Null}" IsTabStop="False" BorderThickness="0" Text="VerticalThumbBrush" VerticalAlignment="Center" Margin="5"/>
        </Border>
        <Border Width="180" Height="30" BorderBrush="Black" BorderThickness="0 0 1 1">
            <TextBox IsReadOnly="True" Style="{x:Null}" IsTabStop="False" BorderThickness="0" Text="VerticalThumbHoverBrush" VerticalAlignment="Center" Margin="5"/>
        </Border>
        <Border Width="180" Height="30" BorderBrush="Black" BorderThickness="0 0 1 1">
            <TextBox IsReadOnly="True" Style="{x:Null}" IsTabStop="False" BorderThickness="0" Text="VerticalThumbPressedBrush" VerticalAlignment="Center" Margin="5"/>
        </Border>
    </ItemsControl>
</UserControl>
