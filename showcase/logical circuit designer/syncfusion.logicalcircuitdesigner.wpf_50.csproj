<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net5.0</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
    <UseWPF>true</UseWPF>
    <EnableDefaultEmbeddedResourceItems>False</EnableDefaultEmbeddedResourceItems>
    <EnableDefaultItems>false</EnableDefaultItems>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <AssemblyName>syncfusion.logicalcircuitdesigner.wpf.app</AssemblyName>
    <RootNamespace>syncfusion.logicalcircuitdesigner.wpf.app</RootNamespace>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DefineConstants>TRACE;NET50</DefineConstants>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DefineConstants>TRACE;NET50</DefineConstants>
  </PropertyGroup>
  <ItemGroup>
    <Compile Remove="obj\**" />
    <Compile Remove="obj_45\**" />
    <Compile Remove="obj_47\**" />
    <Compile Remove="obj_50\**" />
    <EmbeddedResource Remove="obj\**" />
    <EmbeddedResource Remove="obj_45\**" />
    <EmbeddedResource Remove="obj_47\**" />
    <EmbeddedResource Remove="obj_50\**" />
    <None Remove="obj\**" />
    <None Remove="obj_45\**" />
    <None Remove="obj_47\**" />
    <None Remove="obj_50\**" />
    <Page Remove="obj\**" />
    <Page Remove="obj_45\**" />
    <Page Remove="obj_47\**" />
    <Page Remove="obj_50\**" />
    <ProjectReference Include="syncfusion.logicalcircuitdesigner.wpf_lib_50.csproj">
      <Name>syncfusion.logicalcircuitdesigner.wpf_lib_50</Name>
    </ProjectReference>
    <None Remove="Properties\Settings.settings" />
    <None Include="Properties\Settings.settings" />
    <None Remove="App.config" />
    <None Include="App.config" />
    <None Remove="Asset\App.ico" />
    <None Remove="Asset\logicCircuitDiagram.png" />
    <Compile Remove="Model\Symbol.cs" />
    <None Remove="PredefinedDiagram\BasicLogicCircuit.xml" />
    <None Remove="PredefinedDiagram\CustomFullAdder.xml" />
    <None Remove="PredefinedDiagram\FullAdderCircuit.xml" />
    <Compile Remove="ViewModel\LogicGatesViewModel.cs" />
    <Page Remove="Views\LogicCircuitDiagram.xaml" />
    <Compile Remove="Views\LogicCircuitDiagram.xaml.cs" />
    <ApplicationDefinition Include="App.xaml" />
    <Compile Include="App.xaml.cs" />
    <Reference Include="Syncfusion.Compression.Base">
